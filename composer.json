{
    "name": "20tries/date_range",
    "description": "An package for handling date ranges.",
    "keywords": [
        "date", "date_range", "range", "timeperiod", "carbon"
    ],
    "homepage": "https://github.com/20tries/date_range",
    "license": "MIT",
    "require": {
        "php" : ">=5.6",
        "nesbot/carbon": "1.2.*",
        "phpunit/phpunit": "5.2.*"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
